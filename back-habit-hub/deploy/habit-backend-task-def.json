{
    "family": "habit-hub-task-2",
    "networkMode": "bridge",
    "containerDefinitions": [
        {
            "name": "habit-hub",
            "image": "064160141751.dkr.ecr.eu-north-1.amazonaws.com/habit-backend:latest",
            "cpu": 0,
            "memory": 512,
            "memoryReservation": 256,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "POSTGRES_USER",
                    "value": "${POSTGRES_USER}"
                },
                {
                    "name": "POSTGRES_PASSWORD",
                    "value": "${POSTGRES_PASSWORD}"
                },
                {
                    "name": "POSTGRES_PORT",
                    "value": "${POSTGRES_PORT}"
                },
                {
                    "name": "POSTGRES_NAME",
                    "value": "${POSTGRES_NAME}"
                },
                {
                    "name": "POSTGRES_HOST",
                    "value": "${POSTGRES_HOST}"
                },
                {
                    "name": "PORT",
                    "value": "${PORT}"
                },
                {
                    "name": "SECRET",
                    "value": "${SECRET}"
                },
                {
                    "name": "EMAIL_USER",
                    "value": "${EMAIL_USER}"
                },
                {
                    "name": "EMAIL_PASS",
                    "value": "${EMAIL_PASS}"
                },
                {
                    "name": "SALT",
                    "value": "${SALT}"
                },
                {
                    "name": "LOCAL_HOST",
                    "value": "${LOCAL_HOST}"
                },
                {
                    "name": "AWS_ACCESS_KEY_ID",
                    "value": "${AWS_ACCESS_KEY_ID}"
                },
                {
                    "name": "AWS_SECRET_ACCESS_KEY",
                    "value": "${AWS_SECRET_ACCESS_KEY}"
                },
                {
                    "name": "AWS_S3_BUCKET_NAME",
                    "value": "${AWS_S3_BUCKET_NAME}"
                },
                {
                    "name": "AWS_REGION",
                    "value": "${AWS_REGION}"
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/habit-hub-task-2",
                    "awslogs-create-group": "true",
                    "awslogs-region": "eu-north-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "1024",
    "memory": "512"
}